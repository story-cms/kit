import factory from '@adonisjs/lucid/factories';
import { Draft } from '@story-cms/kit';

export const DraftFactory = factory
  .define(Draft, async ({ faker }) => {
    return {
      apiVersion: 1,
      locale: 'en',
      bundle: '[]',
      number: 1,
      storyId: 1,
      feedback: faker.lorem.sentence(),
      status: 'submitted',
    };
  })
  .build();
