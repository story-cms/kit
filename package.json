{
  "name": "@story-cms/kit",
  "version": "1.3.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "histoire dev",
    "todo-lint": "eslint .",
    "clean": "del-cli dist",
    "ui:build": "vite build",
    "backend:build": "tsc -p tsconfig.backend.json",
    "copy:templates": "copyfiles --up 3 \"src/backend/stubs/**/*.stub\" dist/backend/stubs",
    "build": "npm run ui:build && npm run backend:build",
    "prebuild": "npm run clean",
    "postbuild": "npm run copy:templates",
    "prepare": "npm run build",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "test:e2e": "npx playwright test tests/e2e/*.ts",
    "test": "npx playwright test tests/unit/*.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "main": "./dist/backend/index.js",
  "exports": {
    ".": {
      "import": "./dist/backend/index.js",
      "types": "./dist/backend/index.d.ts"
    },
    "./add_meta_noindex_middleware": "./dist/backend/middleware/add_meta_noindex.js",
    "./admin_middleware": "./dist/backend/middleware/admin.js",
    "./cms_provider": "./dist/backend/providers/cms_provider.js",
    "./user": "./dist/backend/models/user.js",
    "./ui": {
      "import": "./dist/frontend/main.js",
      "types": "./dist/frontend/main.d.ts"
    },
    "./styles.css": "./dist/frontend/styles.css",
    "./index.css": "./dist/frontend/index.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/lib-storage": "^3.965.0",
    "@headlessui/vue": "^1.7.23",
    "@inertiajs/vue3": "^2.3.7",
    "@poppinss/utils": "^6.10.1",
    "@rive-app/canvas": "^2.33.3",
    "@types/luxon": "^3.7.1",
    "@vuepic/vue-datepicker": "^12.1.0",
    "easymde": "^2.20.0",
    "luxon": "^3.7.2",
    "marked": "^17.0.1",
    "pinia": "^3.0.4",
    "tus-js-client": "^4.3.1",
    "vue": "^3.5.26"
  },
  "//": {},
  "devDependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/session": "^7.5.1",
    "@eslint/js": "^9.39.2",
    "@google-analytics/data": "^5.2.1",
    "@histoire/plugin-vue": "v1.0.0-beta.1",
    "@playwright/test": "^1.57.0",
    "@swc/core": "^1.15.8",
    "@tailwindcss/forms": "^0.5.11",
    "@types/node": "^22.15.1",
    "@vinejs/vine": "^3.0.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "firebase-admin": "^13.6.0",
    "globals": "^16.0.0",
    "histoire": "v1.0.0-beta.1",
    "openai": "^6.15.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vue-eslint-parser": "^10.2.0",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/session": "^7.5.1",
    "@google-analytics/data": "^5.2.1",
    "@vinejs/vine": "^3.0.1",
    "firebase-admin": "^13.6.0",
    "luxon": "^3.7.2",
    "openai": "^6.15.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "zod": "^4.3.5"
  },
  "tsup": {
    "entry": [
      "./src/backend/index.ts",
      "./src/backend/factories/**/*.ts",
      "./src/backend/middleware/**/*.ts",
      "./src/backend/models/**/*.ts",
      "./src/backend/providers/**/*.ts",
      "./src/backend/services/**/*.ts",
      "./src/backend/validators/**/*.ts"
    ],
    "outDir": "./dist/backend",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  },
  "other": {},
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.25.4",
    "@rollup/rollup-linux-x64-gnu": "^4.40.0"
  }
}
