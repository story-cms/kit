{
  "name": "@story-cms/kit",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "histoire dev",
    "todo-lint": "eslint .",
    "clean": "del-cli dist",
    "ui:build": "vite build",
    "backend:build": "tsc -p tsconfig.backend.json",
    "copy:templates": "copyfiles --up 3 \"src/backend/stubs/**/*.stub\" dist/backend/stubs",
    "build": "npm run ui:build && npm run backend:build",
    "prebuild": "npm run clean",
    "postbuild": "npm run copy:templates",
    "prepare": "npm run build",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "test:e2e": "npx playwright test tests/e2e/*.ts",
    "test": "npx playwright test tests/unit/*.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "main": "./dist/backend/index.js",
  "exports": {
    ".": {
      "import": "./dist/backend/index.js",
      "types": "./dist/backend/index.d.ts"
    },
    "./add_meta_noindex_middleware": "./dist/backend/middleware/add_meta_noindex.js",
    "./admin_middleware": "./dist/backend/middleware/admin.js",
    "./ui": {
      "import": "./dist/frontend/main.js",
      "types": "./dist/frontend/main.d.ts"
    },
    "./styles.css": "./dist/frontend/styles.css",
    "./index.css": "./dist/frontend/index.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.697.0",
    "@aws-sdk/lib-storage": "^3.697.0",
    "@headlessui/vue": "^1.7.22",
    "@inertiajs/vue3": "^2.0.2",
    "@poppinss/utils": "^6.7.3",
    "@rive-app/canvas": "^2.29.0",
    "@types/luxon": "^3.3.0",
    "@vuepic/vue-datepicker": "^11.0.2",
    "easymde": "^2.18.0",
    "luxon": "^3.6.1",
    "marked": "^15.0.3",
    "pinia": "^2.1.7",
    "tus-js-client": "^4.3.1",
    "vue": "^3.4.21"
  },
  "//": {},
  "devDependencies": {
    "@adonisjs/core": "^6.2.3",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/session": "^7.4.2",
    "@eslint/js": "^9.25.1",
    "@histoire/plugin-vue": "^0.17.17",
    "@playwright/test": "^1.49.0",
    "@swc/core": "^1.3.107",
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^22.15.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.0.0",
    "histoire": "^0.17.17",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript-eslint": "^8.31.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.2.1"
  },
  "peerDependencies": {
    "@adonisjs/lucid": "^21.6.1",
    "luxon": "^3.6.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21"
  },
  "tsup": {
    "entry": [
      "./src/backend/index.ts",
      "./src/backend/middleware/admin.ts",
      "./src/backend/middleware/add_meta_noindex.ts"
    ],
    "outDir": "./dist/backend",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  },
  "other": {},
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.25.4",
    "@rollup/rollup-linux-x64-gnu": "^4.40.0"
  }
}